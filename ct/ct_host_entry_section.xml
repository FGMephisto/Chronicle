				<anchored to="innatespells" />
				<static textres="ct_label_innatespells" />
			</label_ct_section>
			
			<subwindow name="spellslots">
				<anchored to="anchor_section">
					<top anchor="bottom" relation="relative" offset="3" />
					<left offset="70" />
					<right offset="-15" />
				</anchored>
			</subwindow>
			<list_ct_section_actions name="spells">
				<datasource>.spells</datasource>
				<script>
					function onListChanged()
						super.onListChanged();

						local bShow = not isEmpty();
						if bShow then
							window.spellslots.setValue("npc_spellslots", window.getDatabaseNode());
						else
							window.spellslots.setValue("", "");
						end
						window.spellslots.setVisible(bShow);
					end
				</script>
			</list_ct_section_actions>
			<label_ct_section name="spells_label">
				<anchored to="spellslots" />
				<static textres="ct_label_spells" />
			</label_ct_section>

			<frame_ct_section name="frame_section">
				<anchored>
					<left parent="icon_section" anchor="right" offset="0" />
					<bottom parent="spells" offset="10" />
				</anchored>
			</frame_ct_section>
		</sheetdata>
