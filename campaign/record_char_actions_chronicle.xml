				</anchored>
				<icon>char_damage</icon>
				<tooltip textres="char_tooltip_dmg" />
			</genericcontrol>

			<!-- For compatibility reasons with 5E we keep "wounds" when damage is meant. Ruleset specific "wounds" are handled via "trauma" -->
